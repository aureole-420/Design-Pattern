                <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_T0fkMrezEeesEL9BuMP45Q" value="*"/>
              </ownedParameter>
              <language>java</language>
              <body>int port = IRMI_Defs.CLASS_SERVER_PORT_SERVER;&#xD;
		String classpath = &quot;&quot;;&#xD;
&#xD;
		if (args.length >= 1) {&#xD;
			port = Integer.parseInt(args[0]);&#xD;
		}&#xD;
&#xD;
		if (args.length >= 2) {&#xD;
			classpath = args[1];&#xD;
		}&#xD;
&#xD;
		try {&#xD;
			new ClassFileServer(port, classpath);&#xD;
		} catch (IOException e) {&#xD;
			System.out.println(&quot;Unable to start ClassServer: &quot; + e.getMessage());&#xD;
			e.printStackTrace();&#xD;
		}&#xD;
	</body>
            </ownedBehavior>
